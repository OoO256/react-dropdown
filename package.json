  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/v6x/react-dropdown/issues"
  },
  "homepage": "https://github.com/v6x/react-dropdown",
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0 || ^16.0.0 || ^17.0.0",
    "react-dom": "^0.14.7 || ^15.0.0-0 || ^16.0.0 || ^17.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
