    "react-dom": "^16.14.0",
    "standard": "^11.0.1",
    "watchify": "^3.11.1"
  },
  "typings": "./index.d.ts",
  "scripts": {
    "build": "babel index.js -o dist/index.js",
    "test": "standard index.js",
